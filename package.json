  "description": "Shopping Cart",
  "author": "Jesper Hansen <jsp_hansen@outlook.com>",
  "scripts": {
    "dev": "cross-env GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) cross-env GIT_VERSION=$(git rev-parse HEAD) cross-env BUILD_DATE=$(date '+%Y-%m-%d_%H:%M:%S') webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js --coverage",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit",
    "build": "cross-env GIT_BRANCH=$(git rev-parse --abbrev-ref HEAD) cross-env GIT_VERSION=$(git rev-parse HEAD) cross-env BUILD_DATE=$(date '+%Y-%m-%d_%H:%M:%S') node build/build.js"
  },
